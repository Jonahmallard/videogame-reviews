<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release Year</th>
        <th>ESRB Rating</th>
        <th>Console</th>
      </tr>
    </thead>
    <tbody>
      <% videogames.forEach(function(g) { %>
        <tr>
          <td><%= g.title %></td>
          <td><%= g.releaseYear %></td>
          <td><%= g.esrbRating %></td>
          <td><%= g.console %></td>
          <td><a href="/videogames/<%= g._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
<%- include('../partials/footer') %>